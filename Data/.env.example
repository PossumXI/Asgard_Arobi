# ASGARD Data Services Environment Variables
# ===========================================================================
# Copy this file to .env in the Data/ directory
# These variables are used by docker-compose.yml
#
# Generate strong passwords with: openssl rand -base64 32
# ===========================================================================

# PostgreSQL
POSTGRES_PASSWORD=your_secure_postgres_password_here

# MongoDB
MONGO_PASSWORD=your_secure_mongo_password_here

# Redis
REDIS_PASSWORD=your_secure_redis_password_here
