{
  "name": "asgard-e2e-tests",
  "version": "1.0.0",
  "description": "End-to-end tests and demo video generation for ASGARD",
  "scripts": {
    "test": "npx playwright test",
    "demo": "npx playwright test demo.spec.ts --headed",
    "demo:record": "npx playwright test demo.spec.ts",
    "demo:complete": "npx playwright test asgard-complete-demo.spec.ts --project=demo-chromium",
    "flight:demo": "npx playwright test flight-simulation-demo.spec.ts --config=playwright-flight-config.ts",
    "flight:demo:headed": "npx playwright test flight-simulation-demo.spec.ts --config=playwright-flight-config.ts --headed",
    "pricilla:demo": "npx playwright test pricilla-demo.spec.ts",
    "report": "npx playwright show-report"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0"
  }
}
